<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.5+</version>
	</compatibility>
	<theme id="lunar-resort" name="Lunar Resort Theme">
		<template-extension>ftl</template-extension>
		
		<settings>
		    <setting configurable="true" key="show-breadcrumbs" 
		    options="true,false" type="select" value="false" />
		    <setting configurable="true" key="portlet-setup-show-borders-default" 
		    options="true,false" type="select" value="true" />
		    <setting configurable="true" key="show-page-title" 
		    options="true,false" type="select" value="true"/>
		    <setting configurable="true" key="show-footer" 
		    options="true, false" type="select" value="true"></setting>
		</settings>
		
	</theme>
</look-and-feel>